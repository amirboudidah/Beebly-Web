<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Propositionlivre" table="propositionlivre">
    <indexes>
      <index name="idcproplivre" columns="idclient"/>
    </indexes>
    <id name="idpropositionlivre" type="integer" column="idPropositionLivre">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titrelivre" type="string" column="titreLivre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="editon" type="string" column="editon" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateproposition" type="date" column="dateProposition" nullable="true"/>
    <field name="descriptionetat" type="string" column="descriptionEtat" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <many-to-one field="idclient" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="idclient" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
